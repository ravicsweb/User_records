
<table>
  <tr>
    <td>First Name
    <td><%= f.text_field :fname %>

  </tr>
  <tr>
    <td>Last Name
    <td><%= f.text_field :lname %>
  </tr>
  <tr>
    <td>Email
    <td><%= f.text_field :email %>
  </tr>
   <tr>
    <td>Cell No.
    <td><%= f.text_field :cell_no %>
  </tr>
  <tr>
    <td> Address
    <td><%= f.text_area :address ,cols: 24, rows: 5%>
  </tr>
  <tr>
    <td>Hobbies
    <td>  <%= check_box_tag 'hobby[]', 'Playing' %>Playing
          <%= check_box_tag 'hobby[]', 'Traveling' %>Traveling
          <%= check_box_tag 'hobby[]', 'Reading' %>Reading
         <!-- <%= check_box_tag 'hobby[]', 'Writting' %>Writting-->
          <%= check_box_tag 'hobby[]', 'Swiming' %>Swiming
          

  </tr>
  <tr>
    <td> City
    <td>
     <%= f.select :city, options_for_select([ "rajkot", "surat","Ahmedabad" ], "Select")%>
  <!--<select name="city">
    <option value="rajkot">rajkot</option>
    <option value="surat">surat</option>
    <option value="ahmedabad">ahmedabad</option>
  </select> -->
  
  </tr>
  <tr>
    <td> Sex
    <td><%=radio_button_tag 'gender', 'male',true%>Male
      <%=radio_button_tag 'gender', 'female'%>Female
     <!-- <input name="sex" type="radio" value="male" checked="true"/>Male
      <input name="sex" type="radio" value="female" />Female-->
      
      
  </tr>
  <tr>
    <td> Date of Birth
    <td>
    <%= f.text_field :bdate%>&nbsp;(MM/DD/YYYY)
      <%#<%=  date_select("bdate", "bd_on", start_year: 1950, end_year: 2014, selected: Date.today, use_month_numbers: true) %>
     
  </tr>
  <tr>
    <td>User Image</td>
    <td><%= f.file_field :avatar %></td>
  </tr>
 
 </table>  



