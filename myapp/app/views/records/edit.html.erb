<h1> Update User Record</h1>
<% if session[:user_id] %>
<%= form_for(@record) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <%= render 'form', :f => f %>
  <div>
    <%= f.submit "Update" %><input type="reset" value="Clear" />
  </div>
<% end %>
<p><%= link_to "Back",records_path %></p>

<% else %>
<p>
  Please Sign in......!
</p> 
<% end %>